Datatype: rdf:PlainLiteral

    
Datatype: xsd:anyURI

    
ObjectProperty: sc:isSuitableToBeDressedInSeason

    Annotations: 
        rdfs:label "Is suitable to be dressed in season"@en,
        rdfs:comment "This property specifies the season for which a particular clothing item and/or class is/are suitable for."@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        dbr:Season
    
    InverseOf: 
        sc:seasonHasSuitableClothingForDressing
    
    
ObjectProperty: sc:hasTextileComposition

    Annotations: 
        rdfs:comment "This property denotes the materials from which a clothing item is composed of."@en,
        rdfs:label "Has textile composition"@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        sc:ClothingMaterial
    
    SubPropertyOf: 
        dbp:composition
    
    
ObjectProperty: sc:seasonHasSuitableClothingForDressing

    Annotations: 
        rdfs:label "Season has suitable clothing for dressing"@en,
        rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular season."@en
    
    Domain: 
        dbr:Season
    
    Range: 
        dbr:Clothing
    
    InverseOf: 
        sc:isSuitableToBeDressedInSeason
    
    
ObjectProperty: sc:isSuitableToBeDressedOnWeather

    Annotations: 
        rdfs:label "Is suitable to be dressed in weather conditions"@en,
        rdfs:comment "This property specifis the weather conditions for which a particular clothing item and/or class is/are suitable for."@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        dbr:Weather
    
    InverseOf: 
        sc:weatherHasSuitableClothingForDressing
    
    
ObjectProperty: dbp:composition

    
ObjectProperty: sc:isManufacteredBy

    Annotations: 
        rdfs:label "Is manufactured by"@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        gr:BusinessEntity
    
    Characteristics: 
        Functional,
        Asymmetric
    
    
ObjectProperty: sc:isSuitableForDressingForReligion

    Annotations: 
        rdfs:comment "This property specifis the religion for which a particular clothing item and/or class is/are suitable for."@en,
        rdfs:label "Is suitable for dressing for religion"@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        dbr:Religion
    
    InverseOf: 
        sc:religionHasSuitableClothingForDressing
    
    
ObjectProperty: sc:religionHasSuitableClothingForDressing

    Annotations: 
        rdfs:comment "This property specifis the clothing that is suitable for a particular religion."@en,
        rdfs:label "Religion has suitable clothing for dressing"@en
    
    Domain: 
        dbr:Religion
    
    Range: 
        dbr:Clothing
    
    InverseOf: 
        sc:isSuitableForDressingForReligion
    
    
ObjectProperty: dbp:dressSize

    Annotations: 
        rdfs:label "Dress size"@en
    
    Domain: 
        sc:Dress
    
    Range: 
        dbr:Clothing_sizes
    
    SubPropertyOf: 
        sc:hasSize
    
    
ObjectProperty: sc:hasColour

    Annotations: 
        rdfs:label "Has colour"@en,
        rdfs:comment "This property specifies the color a particular clothing item has."@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        dbo:Colour
    
    
ObjectProperty: sc:bodyShapeHasSuitableClothingForDressing

    Annotations: 
        rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular body shape."@en,
        rdfs:label "Body shape has suitable clothing for dressing"@en
    
    Domain: 
        sc:BodyShape
    
    Range: 
        dbr:Clothing
    
    InverseOf: 
        sc:isSuitableToBeDressedByBodyShape
    
    
ObjectProperty: sc:eventHasSuitableClothingForDressing

    Annotations: 
        rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular event."@en,
        rdfs:label "Event has suitable clothing for dressing"@en
    
    Domain: 
        dbo:Event
    
    Range: 
        dbr:Clothing
    
    InverseOf: 
        sc:isSuitableToBeDressedAtEvent
    
    
ObjectProperty: sc:isSuitableToBeDressedByGenre

    Annotations: 
        rdfs:label "Is suitable to be dressed by genre"@en,
        rdfs:comment "This property specifies the genre(s) for which a clothing item is suitable for. We need to make this general enough since some clothing items can be suitable for both sexes."@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        dbr:Genre
    
    InverseOf: 
        sc:genreHasSuitableClothingForDressing
    
    
ObjectProperty: sc:genreHasSuitableClothingForDressing

    Annotations: 
        rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular genre."@en,
        rdfs:label "Genre has suitable clothing for dressing"@en
    
    Domain: 
        dbr:Genre
    
    Range: 
        dbr:Clothing
    
    InverseOf: 
        sc:isSuitableToBeDressedByGenre
    
    
ObjectProperty: sc:isSuitableToBeDressedAtEvent

    Annotations: 
        rdfs:comment "This property specifies the Event for which a particular clothing item and/or class is/are suitable for."@en,
        rdfs:label "Is suitable to be dressed at event"@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        dbo:Event
    
    InverseOf: 
        sc:eventHasSuitableClothingForDressing
    
    
ObjectProperty: sc:dressingCodeHasSuitableClothingForDressing

    Annotations: 
        rdfs:label "Dressing code has suitable clothing for dressing"@en,
        rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular dressing code."@en
    
    Domain: 
        dbr:Dress_code,
        sc:ClothingStyle
    
    Range: 
        dbr:Clothing
    
    InverseOf: 
        sc:isSuitableForDressingCode
    
    
ObjectProperty: sc:isSuitableForDressingCode

    Annotations: 
        rdfs:comment "This property specifis the dressing code for which a particular clothing item and/or class is/are suitable for."@en,
        rdfs:label "Is suitable for dressing code"@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        sc:ClothingStyle,
        dbr:Dress_code
    
    InverseOf: 
        sc:dressingCodeHasSuitableClothingForDressing
    
    
ObjectProperty: sc:hasTexture

    Annotations: 
        rdfs:comment "Specifies the texture of the clothing item."@en,
        rdfs:label "Has texture"
    
    Domain: 
        dbr:Clothing
    
    Range: 
        sc:ClothingTexture
    
    
ObjectProperty: sc:isSuitableToBeDressedByBodyShape

    Annotations: 
        rdfs:label "Is suitable to be dressed by body shape"@en,
        rdfs:comment "Stipulates for which body shape a particular clothing item and/or class is/are suitable for."@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        sc:BodyShape
    
    InverseOf: 
        sc:bodyShapeHasSuitableClothingForDressing
    
    
ObjectProperty: sc:weatherHasSuitableClothingForDressing

    Annotations: 
        rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular weather condition."@en,
        rdfs:label "Weather has suitable clothing for dressing"@en
    
    Domain: 
        dbr:Weather
    
    Range: 
        dbr:Clothing
    
    InverseOf: 
        sc:isSuitableToBeDressedOnWeather
    
    
ObjectProperty: sc:hasSize

    Annotations: 
        rdfs:comment "This property specifies the size of a particular clothing item."@en,
        rdfs:label "Has size"@en
    
    Domain: 
        dbr:Clothing
    
    Range: 
        dbr:Clothing_sizes
    
    Characteristics: 
        Functional
    
    
