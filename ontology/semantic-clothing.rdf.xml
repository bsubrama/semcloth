    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY xml "http://www.w3.org/XML/1998/namespace" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY sc "http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#" >
]>


<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#"
     xml:base="http://www.semanticweb.org/ontologies/2015/02/semcloth.owl"
     xmlns:sc="http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#"
     xmlns:dbo="http://dbpedia.org/ontology/"
     xmlns:dbp="http://dbpedia.org/property/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:dbr="http://dbpedia.org/resource/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth.owl">
        <rdfs:label rdf:datatype="&xsd;string">A common vocabular for clothing items.</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Semantic Clothing Ontology</rdfs:label>
        <dc:date rdf:datatype="&xsd;dateTime">2015</dc:date>
        <dc:creator rdf:datatype="&xsd;string">Daniel Amariei, Oana Gagea, Iordan Rață.</dc:creator>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="&dc;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/property/composition -->

    <owl:ObjectProperty rdf:about="&dbp;composition"/>
    


    <!-- http://dbpedia.org/property/dressSize -->

    <owl:ObjectProperty rdf:about="&dbp;dressSize">
        <rdfs:subPropertyOf rdf:resource="&sc;hasSize"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasColour -->

    <owl:ObjectProperty rdf:about="&sc;hasColour">
        <rdfs:label rdf:datatype="&xsd;string">Has colour</rdfs:label>
        <rdfs:range rdf:resource="&dbr;Color"/>
        <rdfs:domain rdf:resource="&sc;Clothing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasSize -->

    <owl:ObjectProperty rdf:about="&sc;hasSize">
        <rdfs:label rdf:datatype="&xsd;string">Has size</rdfs:label>
        <rdfs:range rdf:resource="&dbr;Clothing_sizes"/>
        <rdfs:range rdf:resource="&dbr;Shoe_size"/>
        <rdfs:domain rdf:resource="&sc;Clothing"/>
        <rdfs:domain rdf:resource="&sc;Shoes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasTextileComposition -->

    <owl:ObjectProperty rdf:about="&sc;hasTextileComposition">
        <rdfs:label rdf:datatype="&xsd;string">Has textile composition</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property denotes the materials from which a clothing item is composed of.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dbp;composition"/>
        <rdfs:domain rdf:resource="&sc;Clothing"/>
        <rdfs:range rdf:resource="&sc;ClothingMaterial"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForBodyShape -->

    <owl:ObjectProperty rdf:about="&sc;isSuitableForBodyShape">
        <rdfs:label rdf:datatype="&xsd;string">Is suitable for body shape</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForEvent -->

    <owl:ObjectProperty rdf:about="&sc;isSuitableForEvent">
        <rdfs:label rdf:datatype="&xsd;string">Is suitable for event</rdfs:label>
        <rdfs:range rdf:resource="&dbo;Event"/>
        <rdfs:domain rdf:resource="&sc;Clothing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForGenre -->

    <owl:ObjectProperty rdf:about="&sc;isSuitableForGenre">
        <rdfs:label rdf:datatype="&xsd;string">Is suitable for genre</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property specifies the genre(s) for which a clothing item is suitable for. We need to make this general enough since some clothing items can be suitable for both sexes</rdfs:comment>
        <rdfs:range rdf:resource="&dbr;Genre"/>
        <rdfs:domain rdf:resource="&sc;Clothing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForSeason -->

    <owl:ObjectProperty rdf:about="&sc;isSuitableForSeason">
        <rdfs:label rdf:datatype="&xsd;string">Is suitable for season</rdfs:label>
        <rdfs:range rdf:resource="&dbr;Season"/>
        <rdfs:domain rdf:resource="&sc;Clothing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForWeather -->

    <owl:ObjectProperty rdf:about="&sc;isSuitableForWeather">
        <rdfs:label rdf:datatype="&xsd;string">Is suitable for weather conditions</rdfs:label>
        <rdfs:range rdf:resource="&dbr;Weather"/>
        <rdfs:domain rdf:resource="&sc;Clothing"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/ontology/Event -->

    <owl:Class rdf:about="&dbo;Event"/>
    


    <!-- http://dbpedia.org/resource/Clothing_sizes -->

    <owl:Class rdf:about="&dbr;Clothing_sizes"/>
    


    <!-- http://dbpedia.org/resource/Color -->

    <owl:Class rdf:about="&dbr;Color"/>
    


    <!-- http://dbpedia.org/resource/Genre -->

    <owl:Class rdf:about="&dbr;Genre"/>
    


    <!-- http://dbpedia.org/resource/Raw_material -->

    <owl:Class rdf:about="&dbr;Raw_material"/>
    


    <!-- http://dbpedia.org/resource/Season -->

    <owl:Class rdf:about="&dbr;Season"/>
    


    <!-- http://dbpedia.org/resource/Shoe_size -->

    <owl:Class rdf:about="&dbr;Shoe_size"/>
    


    <!-- http://dbpedia.org/resource/Weather -->

    <owl:Class rdf:about="&dbr;Weather">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Accessory -->

    <owl:Class rdf:about="&sc;Accessory">
        <rdfs:label rdf:datatype="&xsd;string">Accessory</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#AnkleBoots -->

    <owl:Class rdf:about="&sc;AnkleBoots">
        <rdfs:label rdf:datatype="&xsd;string">Ankle boots</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Shoes"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bag -->

    <owl:Class rdf:about="&sc;Bag">
        <rdfs:label rdf:datatype="&xsd;string">Bag</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Accessory"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Belt -->

    <owl:Class rdf:about="&sc;Belt">
        <rdfs:label rdf:datatype="&xsd;string">Belt</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Accessory"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Blazer -->

    <owl:Class rdf:about="&sc;Blazer">
        <rdfs:label rdf:datatype="&xsd;string">Blazer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bodysuit -->

    <owl:Class rdf:about="&sc;Bodysuit">
        <rdfs:label rdf:datatype="&xsd;string">Bodysuit</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Lingerie"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Boots -->

    <owl:Class rdf:about="&sc;Boots">
        <rdfs:label rdf:datatype="&xsd;string">Boots</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Shoes"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bra -->

    <owl:Class rdf:about="&sc;Bra">
        <rdfs:label rdf:datatype="&xsd;string">Bra</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Lingerie"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bracelet -->

    <owl:Class rdf:about="&sc;Bracelet">
        <rdfs:label rdf:datatype="&xsd;string">Bracelet</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Jewellery"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Briefs -->

    <owl:Class rdf:about="&sc;Briefs">
        <rdfs:label rdf:datatype="&xsd;string">Briefs</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Lingerie"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#BusinessMeeting -->

    <owl:Class rdf:about="&sc;BusinessMeeting">
        <rdfs:label rdf:datatype="&xsd;string">Business meeting</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dbo;Event"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Clothing -->

    <owl:Class rdf:about="&sc;Clothing">
        <rdfs:label rdf:datatype="&xsd;string">Clothing</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Represents a clothing item (e.g. Skirt, Jeans, Shoes, etc.) or a collection of clothing items (e.g. a suit -- which is formed from multiple items.). We need to abstract the posibility that a clothing item can be formed from one or multiple clothing items.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingCharacteristic -->

    <owl:Class rdf:about="&sc;ClothingCharacteristic">
        <rdfs:label rdf:datatype="&xsd;string">Clothing characteristic</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingMaterial -->

    <owl:Class rdf:about="&sc;ClothingMaterial">
        <rdfs:label rdf:datatype="&xsd;string">Clothing material</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dbr;Raw_material"/>
        <rdfs:comment rdf:datatype="&xsd;string">The basic materials from which clothes are made of.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingsWithAllNaturalMaterials -->

    <owl:Class rdf:about="&sc;ClothingsWithAllNaturalMaterials">
        <rdfs:label>Clothings with all natural materials.</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sc;hasTextileComposition"/>
                <owl:allValuesFrom rdf:resource="&sc;NaturalClothingMaterial"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingsWithAllSyntheticMaterials -->

    <owl:Class rdf:about="&sc;ClothingsWithAllSyntheticMaterials">
        <rdfs:label>Clothings with all syntetic materials.</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sc;hasTextileComposition"/>
                <owl:allValuesFrom rdf:resource="&sc;SyntheticClothingMaterial"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingsWithAtLeastOneNaturalMaterial -->

    <owl:Class rdf:about="&sc;ClothingsWithAtLeastOneNaturalMaterial">
        <rdfs:label>Clothings with at least one natural material.</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sc;hasTextileComposition"/>
                <owl:someValuesFrom rdf:resource="&sc;NaturalClothingMaterial"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingsWithAtLeastOneSyntheticMaterial -->

    <owl:Class rdf:about="&sc;ClothingsWithAtLeastOneSyntheticMaterial">
        <rdfs:label>Clothings with at least one synthetic material.</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sc;hasTextileComposition"/>
                <owl:someValuesFrom rdf:resource="&sc;SyntheticClothingMaterial"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Club -->

    <owl:Class rdf:about="&sc;Club">
        <rdfs:label rdf:datatype="&xsd;string">Club</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dbo;Event"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Coat -->

    <owl:Class rdf:about="&sc;Coat">
        <rdfs:label rdf:datatype="&xsd;string">Coat</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Date -->

    <owl:Class rdf:about="&sc;Date">
        <rdfs:label rdf:datatype="&xsd;string">Date</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dbo;Event"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Dress -->

    <owl:Class rdf:about="&sc;Dress">
        <rdfs:label rdf:datatype="&xsd;string">Dress</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Earrings -->

    <owl:Class rdf:about="&sc;Earrings">
