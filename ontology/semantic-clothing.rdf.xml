    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Funneral -->

    <owl:Class rdf:about="&sc;Funneral">
        <rdfs:label rdf:datatype="&xsd;string">Funneral</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dbo;Event"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Gloves -->

    <owl:Class rdf:about="&sc;Gloves">
        <rdfs:label rdf:datatype="&xsd;string">Gloves</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Handbag -->

    <owl:Class rdf:about="&sc;Handbag">
        <rdfs:label rdf:datatype="&xsd;string">Handbag</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Bag"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Hat -->

    <owl:Class rdf:about="&sc;Hat">
