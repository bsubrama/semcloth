        <rdfs:subClassOf rdf:resource="&sc;Jewellery"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Flats -->

    <owl:Class rdf:about="&sc;Flats">
