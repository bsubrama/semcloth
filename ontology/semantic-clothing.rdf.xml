        <rdfs:subClassOf rdf:resource="&sc;Underwear"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#MensClothing -->

    <owl:Class rdf:about="&sc;MensClothing">
        <rdfs:label rdf:datatype="&xsd;string">Men&apos;s clothing</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sc;isSuitableForGenre"/>
                <owl:hasValue rdf:resource="&dbr;Male"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Moccasins -->

    <owl:Class rdf:about="&sc;Moccasins">
        <rdfs:label rdf:datatype="&xsd;string">Mocassins</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Shoes"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#NaturalClothingMaterial -->

    <owl:Class rdf:about="&sc;NaturalClothingMaterial">
        <rdfs:label rdf:datatype="&xsd;string">Natural clothing material</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;ClothingMaterial"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#NaturalMaterialClothing -->

    <owl:Class rdf:about="&sc;NaturalMaterialClothing">
        <rdfs:label rdf:datatype="&xsd;string">Natural material clothing</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&sc;ClothingsWithAllNaturalMaterials"/>
                    <rdf:Description rdf:about="&sc;ClothingsWithAtLeastOneNaturalMaterial"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Necklace -->

    <owl:Class rdf:about="&sc;Necklace">
