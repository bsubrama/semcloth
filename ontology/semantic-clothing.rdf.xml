        <rdfs:subClassOf rdf:resource="&sc;Accessory"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#HighHeels -->

    <owl:Class rdf:about="&sc;HighHeels">
        <rdfs:label rdf:datatype="&xsd;string">High-heels</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Shoes"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Interview -->

    <owl:Class rdf:about="&sc;Interview">
        <rdfs:label rdf:datatype="&xsd;string">Interview</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;BusinessMeeting"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Jeans -->

    <owl:Class rdf:about="&sc;Jeans">
        <rdfs:label rdf:datatype="&xsd;string">Jeans</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Clothing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Jewellery -->

    <owl:Class rdf:about="&sc;Jewellery">
        <rdfs:label rdf:datatype="&xsd;string">Jewellery</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Accessory"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Knickers -->

    <owl:Class rdf:about="&sc;Knickers">
        <rdfs:label rdf:datatype="&xsd;string">Knickers</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sc;Lingerie"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Lingerie -->

    <owl:Class rdf:about="&sc;Lingerie">
