###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Clothing

:Clothing rdf:type owl:Class ;
          
          rdfs:label "Clothing"^^xsd:string ;
          
          rdfs:comment "Represents a clothing item (e.g. Skirt, Jeans, Shoes, etc.) or a collection of clothing items (e.g. a suit -- which is formed from multiple items.). We need to abstract the posibility that a clothing item can be formed from one or multiple clothing items."^^xsd:string .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingCharacteristic

:ClothingCharacteristic rdf:type owl:Class ;
                        
                        rdfs:label "Clothing characteristic"^^xsd:string .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingMaterial

:ClothingMaterial rdf:type owl:Class ;
                  
                  rdfs:label "Clothing material"^^xsd:string ;
                  
                  rdfs:subClassOf dbr:Raw_material ;
                  
                  rdfs:comment "The basic materials from which clothes are made of."^^xsd:string .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingsWithAllNaturalMaterials

:ClothingsWithAllNaturalMaterials rdf:type owl:Class ;
                                  
                                  rdfs:label "Clothings with all natural materials." ;
                                  
                                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasTextileComposition ;
                                                        owl:allValuesFrom :NaturalClothingMaterial
                                                      ] ;
                                  
                                  rdfs:subClassOf :Clothing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingsWithAllSyntheticMaterials

:ClothingsWithAllSyntheticMaterials rdf:type owl:Class ;
                                    
                                    rdfs:label "Clothings with all syntetic materials." ;
                                    
                                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasTextileComposition ;
                                                          owl:allValuesFrom :SyntheticClothingMaterial
                                                        ] ;
                                    
                                    rdfs:subClassOf :Clothing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingsWithAtLeastOneNaturalMaterial

:ClothingsWithAtLeastOneNaturalMaterial rdf:type owl:Class ;
                                        
                                        rdfs:label "Clothings with at least one natural material." ;
                                        
                                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasTextileComposition ;
                                                              owl:someValuesFrom :NaturalClothingMaterial
                                                            ] ;
                                        
                                        rdfs:subClassOf :Clothing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingsWithAtLeastOneSyntheticMaterial

:ClothingsWithAtLeastOneSyntheticMaterial rdf:type owl:Class ;
                                          
                                          rdfs:label "Clothings with at least one synthetic material." ;
                                          
                                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasTextileComposition ;
                                                                owl:someValuesFrom :SyntheticClothingMaterial
                                                              ] ;
                                          
                                          rdfs:subClassOf :Clothing .



