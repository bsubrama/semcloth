# Natural clothing materials
dbr:Cotton a :NaturalClothingMaterial .

dbr:Leather a :NaturalClothingMaterial .

dbr:Wool a :NaturalClothingMaterial .

dbr:Fur a :NaturalClothingMaterial .

dbr:Linen a :NaturalClothingMaterial .

dbr:Mohair a :NaturalClothingMaterial .

dbr:Silk a :NaturalClothingMaterial .

dbr:Lace a :NaturalClothingMaterial .

dbr:Cashmere_wool a :NaturalClothingMaterial .


#Syntetic clothing materials
dbr:Spandex a :SyntheticClothingMaterial ;
			owl:sameAs dbr:Lycra, dbr:Elastan .
			
dbr:Polyester a :SyntheticClothingMaterial .
			
dbr:Acrylic_fiber a :SyntheticClothingMaterial .

dbr:Satin a :SyntheticClothingMaterial .

dbr:Nylon a :SyntheticClothingMaterial .


:ClothingCharacteristic a owl:Class ;
					    rdfs:label "Clothing characteristic"^^xsd:string .

dbr:Color a :ClothingCharacteristic .

dbr:Clothing_sizes a :ClothingCharacteristic .

dbr:Shoe_size a :ClothingCharacteristic .


# Weather
dbr:Weather a owl:Class ;
			rdfs:subClassOf owl:Thing .
								    
dbr:Cold a dbr:Weather .

:Hot a dbr:Weather ;
     rdfs:label "Hot"^^xsd:string .
					    
:Wet a dbr:Weather ;
     rdfs:label "Wet"^^xsd:string .

:Dry a dbr:Weather ;
     rdfs:label "Dry"^^xsd:string .
     
:Clear a dbr:Weather ;
       rdfs:label "Clear"^^xsd:string .
       
dbr:cloudy a dbr:Weather ;
      	   rdfs:label "Cloudy"^^xsd:string .
       

# The genres
dbr:Genre a owl:Class .

dbr:Female a dbr:Genre .

dbr:Male a dbr:Genre .

# Infering clothing types

						   
:UnisexClothing a owl:Class ;
				rdfs:subClassOf :Clothing ;
				owl:equivalentClass [ rdfs:subClassOf :Clothing ;
									  owl:intersectionOf (:MensClothing :WomensClothing)
									] ;
									
				rdfs:label "Unisex clothing"^^xsd:string .

:WomensClothing a owl:Class ;
				rdfs:subClassOf :Clothing ;
				owl:equivalentClass [ a owl:Restriction ;
									  owl:onProperty :isSuitableForGenre ;
									  owl:hasValue dbr:Female 
									] ;
				rdfs:label "Women's clothing"^^xsd:string .
				

:MensClothing a owl:Class ;
			  rdfs:subClassOf :Clothing ;
			  owl:equivalentClass [ a owl:Restriction ;
									  owl:onProperty :isSuitableForGenre ;
									  owl:hasValue dbr:Male 
								  ] ;
				rdfs:label "Men's clothing"^^xsd:string .





# The properties of the Semantic Clothing vocabulary

:hasTextileComposition a owl:ObjectProperty ;
	            	   rdfs:subPropertyOf dbp:composition ;
	            	   rdfs:domain :Clothing ;
	            	   rdfs:range :ClothingMaterial ;
	            	   rdfs:label "Has textile composition"^^xsd:string ;
	            	   rdfs:comment "This property denotes the materials from which a clothing item is composed of."^^xsd:string .
	            	   
	            	   
:hasColour a owl:ObjectProperty ;
		   owl:sameAs dbp:colour ;
    	   rdfs:domain :Clothing ;
    	   rdfs:range :Color ;
		   rdfs:label "Has colour"^^xsd:string .

# To consider making it an DatatypeProperty
:hasSize a owl:ObjectProperty ;
         rdfs:domain :Clothing ;
	     rdfs:range :Clothing_sizes ;
		 rdfs:label "Has size"^^xsd:string .

dbp:dressSize rdfs:subPropertyOf :hasSize ;
			  rdfs:domain :Dress ;
			  rdfs:range :Clothing_sizes ;
			  rdfs:label "Dress size"^^xsd:string .

:isSuitableForGenre a owl:ObjectProperty ;
             	    rdfs:domain :Clothing ;
             	    rdfs:range dbr:Genre ;
					rdfs:label "Is suitable for genre"^^xsd:string ;
					rdfs:comment "This property specifies the genre(s) for which a clothing item is suitable for. We need to make this general enough since some clothing items can be suitable for both sexes"^^xsd:string .

:isSuitableForEvent a owl:ObjectProperty ;
             	    rdfs:domain :Clothing ;
             	    rdfs:range dbo:Event ;
					rdfs:label "Is suitable for event"^^xsd:string .

:isSuitableForBodyShape a owl:ObjectProperty ;
					    rdfs:label "Is suitable for body shape"^^xsd:string .
					    
:isSuitableForSeason a owl:ObjectProperty ;
             	     rdfs:domain :Clothing ;
             	     rdfs:range dbr:Season ;
   				     rdfs:label "Is suitable for season"^^xsd:string .

:isSuitableForWeather a owl:ObjectProperty ;
					  rdfs:domain :Clothing ;
					  rdfs:range :Weather ;						  
					  rdfs:label "Is suitable for weather conditions"^^xsd:string .

:isSuitableForSeason a owl:ObjectProperty ;
					 rdfs:domain :Clothing ;
					 rdfs:range dbr:Season ;
					 rdfs:label "Is suitable for Season"^^xsd:string .
					    
					    
