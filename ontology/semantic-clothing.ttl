:Briefs rdf:type owl:Class ;
        
        rdfs:label "Briefs"^^xsd:string ;
        
        rdfs:subClassOf :Lingerie ;
        
        dbo:thumbnail "http://students.info.uaic.ro/~oana.gagea/images-wade/briefs.png"^^xsd:anyURI .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#BusinessMeeting

:BusinessMeeting rdf:type owl:Class ;
                 
                 rdfs:label "Business meeting"^^xsd:string ;
                 
                 rdfs:subClassOf dbo:Event .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingCharacteristic

:ClothingCharacteristic rdf:type owl:Class ;
                        
                        rdfs:label "Clothing characteristic"^^xsd:string ;
                        
                        owl:disjointUnionOf ( dbr:Clothing_sizes
                                              dbo:Colour
                                              :BodyShape
                                              :ClothingMaterial
                                              :ClothingStyle
                                              :ClothingTexture
                                            ) .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingMaterial

:ClothingMaterial rdf:type owl:Class ;
                  
                  rdfs:label "Clothing material"^^xsd:string ;
                  
                  rdfs:subClassOf dbr:Raw_material ;
                  
                  rdfs:comment "The basic materials from which clothes are made of."^^xsd:string ;
                  
                  owl:disjointUnionOf ( :NaturalClothingMaterial
                                        :SyntheticClothingMaterial
                                      ) .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingStyle

:ClothingStyle rdf:type owl:Class ;
               
               rdfs:label "Clothing style"^^xsd:string ;
               
               rdfs:comment "This property specifies the clothing style for which a particular clothing item and/or class is/are suitable for."^^xsd:string .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingTexture

:ClothingTexture rdf:type owl:Class ;
                 
                 rdfs:label "Clothing texture"^^xsd:string .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Club
:Club rdf:type owl:Class ;

      
      rdfs:label "Club"^^xsd:string ;
      
      rdfs:subClassOf dbo:Event .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Coat

:Coat rdf:type owl:Class ;
      
      rdfs:label "Coat"^^xsd:string ;
      
      rdfs:subClassOf dbr:Clothing ;
      
      dbo:thumbnail "http://students.info.uaic.ro/~oana.gagea/images-wade/coat.png"^^xsd:anyURI .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Date

:Date rdf:type owl:Class ;
      
      rdfs:label "Date"^^xsd:string ;
      
      rdfs:subClassOf dbo:Event .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#DinnerParty

:DinnerParty rdf:type owl:Class ;
             
             rdfs:label "Dinner party"^^xsd:string ;
             
             rdfs:subClassOf dbo:Event .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Dress

:Dress rdf:type owl:Class ;
       
       rdfs:label "Dress"^^xsd:string ;
       
       rdfs:subClassOf dbr:Clothing ;
       
       dbo:thumbnail "http://students.info.uaic.ro/~oana.gagea/images-wade/dress.png"^^xsd:anyURI .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Earrings

:Earrings rdf:type owl:Class ;
          
          rdfs:label "Earrings"^^xsd:string ;
          
          rdfs:subClassOf :Jewellery ;
          
          dbo:thumbnail "http://students.info.uaic.ro/~oana.gagea/images-wade/earrings.png"^^xsd:anyURI .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Flats

:Flats rdf:type owl:Class ;
       
       rdfs:label "Flats"^^xsd:string ;
       
       rdfs:subClassOf :Shoes .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Funneral

:Funneral rdf:type owl:Class ;
          
          rdfs:label "Funneral"^^xsd:string ;
          
          rdfs:subClassOf dbo:Event .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#FunneralClothing

:FunneralClothing rdf:type owl:Class ;
                  
                  rdfs:label "Funneral clothing"^^xsd:string ;
                  
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasColour ;
                                                               owl:hasValue dbr:Black
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :isSuitableToBeDressedAtEvent ;
                                                               owl:hasValue :Funneral
                                                             ]
                                                           )
                                      ] ;
                  
                  rdfs:subClassOf dbr:Clothing ;
                  
                  rdfs:comment "Clothing that is suitable for a funneral."^^xsd:string .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Gloves

:Gloves rdf:type owl:Class ;
        
