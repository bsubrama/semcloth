       rdfs:subClassOf :Shoes ;
       
       dbo:thumbnail "http://students.info.uaic.ro/~oana.gagea/images-wade/boots.png"^^xsd:anyURI .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bra

:Bra rdf:type owl:Class ;
     
     rdfs:label "Bra"^^xsd:string ;
     
     rdfs:subClassOf :Lingerie ;
     
