# The materials from which clothes are made of
:ClothingMaterial a owl:Class, :ClothingCharacteristic ;
				  rdfs:subClassOf dbr:Raw_material ;
				  rdfs:label "Clothing material"^^xsd:string ;
				  rdfs:comment "The basic materials from which clothes are made of."^^xsd:string .

:NaturalClothingMaterial a owl:Class ; 
		                 rdfs:subClassOf :ClothingMaterial ;
						 rdfs:label "Natural clothing material"^^xsd:string .
          
:SyntheticClothingMaterial a owl:Class ;
						   rdfs:subClassOf :ClothingMaterial ;
						   rdfs:label "Synthetic clothing material"^^xsd:string .

:NaturalClothingMaterial owl:disjointWith :SyntheticClothingMaterial .

