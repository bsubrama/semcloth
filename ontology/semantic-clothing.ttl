:Blazer rdf:type owl:Class ;
        
        rdfs:label "Blazer"^^xsd:string ;
        
        rdfs:subClassOf dbr:Clothing ;
        
        dbo:thumbnail "http://students.info.uaic.ro/~oana.gagea/images-wade/blazer-1.png"^^xsd:anyURI .



