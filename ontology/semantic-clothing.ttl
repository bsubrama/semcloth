<http://www.semanticweb.org/ontologies/2015/02/semcloth.owl> 
					rdf:type owl:Ontology ;
                    rdfs:label "Semantic Clothing Ontology"^^xsd:string ,
							   "A common vocabular for clothing items."^^xsd:string ;											  
                    dc:date "2015"^^xsd:dateTime ;
                    dc:creator "Daniel Amariei, Oana Gagea, Iordan Rață."^^xsd:string .
                                         
                                         
# Annotation properties                                                           

dc:creator a owl:AnnotationProperty .
dc:date a owl:AnnotationProperty .


# The classes of the Semantic Clothing vocabulary

# Defining the events


:BusinessMeeting a owl:Class ;
				 rdfs:subClassOf dbo:Event ;
		 		 rdfs:label "Business meeting"^^xsd:string .


:Interview a owl:Class ;
 		   rdfs:subClassOf :BusinessMeeting ;
		   rdfs:label "Interview"^^xsd:string .
		  
:Wedding a owl:Class ;
 		 rdfs:subClassOf dbo:Event ;
		 rdfs:label "Wedding"^^xsd:string .

:PyjamaParty a owl:Class ;
 		     rdfs:subClassOf dbo:Event ;
		     rdfs:label "Pyjama party"^^xsd:string .

:Funneral a owl:Class ;
          rdfs:subClassOf dbo:Event ;
		  rdfs:label "Funneral"^^xsd:string .
		 
:Date a owl:Class ;
      rdfs:subClassOf dbo:Event ;
	  rdfs:label "Date"^^xsd:string .
	  
:RomanticDate a owl:Class ;
			  rdfs:subClassOf :Date ;
 			  rdfs:label "Romantic date"^^xsd:string .

:Club rdfs:subClassOf dbo:Event ;
 		rdfs:label "Club"^^xsd:string .
		 
# The clothing items

:Clothing a owl:Class ;
		  rdfs:label "Clothing"^^xsd:string ;
		  rdfs:comment "Represents a clothing item (e.g. Skirt, Jeans, Shoes, etc.) or a collection of clothing items (e.g. a suit -- which is formed from multiple items.). We need to abstract the posibility that a clothing item can be formed from one or multiple clothing items."^^xsd:string .		  

:Coat a owl:Class ;
      rdfs:subClassOf :Clothing ;
   	  rdfs:label "Coat"^^xsd:string .

:Blazer a owl:Class ;
        rdfs:subClassOf :Clothing ;
   		rdfs:label "Blazer"^^xsd:string .

