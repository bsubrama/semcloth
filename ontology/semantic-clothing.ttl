<http://www.semanticweb.org/ontologies/2015/02/semcloth.owl> rdf:type owl:Ontology ;
                                                             
                                                             rdfs:label "A common vocabulary for clothing items."^^xsd:string ,
                                                                        "Semantic Clothing Ontology"^^xsd:string ;
                                                             
                                                             dc:bibliographicCitation "*** H&M, http://www.hm.com/ro/ (provided support for a few of the concepts used in this ontology)"^^xsd:string ,
                                                                                      "***, Dr. Sabin-Corneliu Buraga, Web Applications Development (Fall 2014), (RO) http://profs.info.uaic.ro/~busaco/teach/courses/wade/index.html, UAIC, FII (this resource provided serveral useful examples from which this ontolgy was modeled after, together with theoretical support)."^^xsd:string ,
                                                                                      "***, Haine Grasute, http://hainegrasute.blogspot.ro/2013/05/cum-sa-te-imbraci-avantajos-in-functie.html (provided support for modelling the 'body shapes' part of the ontology)"^^xsd:string ,
                                                                                      "***, W3C, http://www.w3.org/2001/sw/"^^xsd:string ,
                                                                                      "***, ZARA, http://www.zara.com/ro/en/ (provided support for serveral concepts used in this ontology)"^^xsd:string ;
                                                             
                                                             dc:date "2015-02"^^xsd:gYearMonth ;
                                                             
                                                             dc:creator "Daniel Amariei"^^xsd:string ;
                                                             
                                                             dc:contributor "Ioana Bîrsan"^^xsd:string ,
                                                                            "Iordan Rață"^^xsd:string ,
                                                                            "Oana Gagea"^^xsd:string ;
                                                             
                                                             rdfs:comment "The purpose of this ontology is to create the common conceptual framwork for 'talking' about clothing or related concepts."^^xsd:string .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://dbpedia.org/ontology/thumbnail

dbo:thumbnail rdf:type owl:AnnotationProperty .



###  http://purl.org/dc/elements/1.1/creator

dc:creator rdf:type owl:AnnotationProperty .



###  http://purl.org/dc/elements/1.1/date

dc:date rdf:type owl:AnnotationProperty .



###  http://rdfs.org/sioc/ns#note

sioc:note rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2000/01/rdf-schema#comment

rdfs:comment rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2000/01/rdf-schema#domain

rdfs:domain rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://dbpedia.org/property/composition

dbp:composition rdf:type owl:ObjectProperty .



###  http://dbpedia.org/property/dressSize

dbp:dressSize rdf:type owl:ObjectProperty ;
              
              rdfs:label "Dress size"^^xsd:string ;
              
              rdfs:range dbr:Clothing_sizes ;
              
              rdfs:domain :Dress ;
              
              rdfs:subPropertyOf :hasSize .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#bodyShapeHasSuitableClothingForDressing

:bodyShapeHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                         
                                         rdfs:label "Body shape has suitable clothing for dressing"^^xsd:string ;
                                         
                                         rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular body shape."^^xsd:string ;
                                         
                                         rdfs:range dbr:Clothing ;
                                         
                                         rdfs:domain :BodyShape .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#dressingCodeHasSuitableClothingForDressing

:dressingCodeHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                            
                                            rdfs:label "Dressing code has suitable clothing for dressing"^^xsd:string ;
                                            
                                            rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular dressing code."^^xsd:string ;
                                            
                                            rdfs:range dbr:Clothing ;
                                            
                                            rdfs:domain dbr:Dress_code ,
                                                        :ClothingStyle .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#eventHasSuitableClothingForDressing

:eventHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                     
                                     rdfs:label "Event has suitable clothing for dressing"^^xsd:string ;
                                     
                                     rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular event."^^xsd:string ;
                                     
                                     rdfs:domain dbo:Event ;
                                     
                                     rdfs:range dbr:Clothing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#genreHasSuitableClothingForDressing

:genreHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                     
                                     rdfs:label "Genre has suitable clothing for dressing"^^xsd:string ;
                                     
                                     rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular genre."^^xsd:string ;
                                     
                                     rdfs:range dbr:Clothing ;
                                     
                                     rdfs:domain dbr:Genre .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasColour

:hasColour rdf:type owl:ObjectProperty ;
           
           rdfs:label "Has colour"^^xsd:string ;
           
           rdfs:comment "This property specifies the color a particular clothing item has."^^xsd:string ;
           
           rdfs:domain dbr:Clothing ;
           
           rdfs:range dbo:Colour .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasSize

:hasSize rdf:type owl:FunctionalProperty ,
                  owl:ObjectProperty ;
         
         rdfs:label "Has size"^^xsd:string ;
         
         rdfs:comment "This property specifies the size of a particular clothing item."^^xsd:string ;
         
         rdfs:domain dbr:Clothing ;
         
         rdfs:range dbr:Clothing_sizes .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasTextileComposition

:hasTextileComposition rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "Has textile composition"^^xsd:string ;
                       
                       rdfs:comment "This property denotes the materials from which a clothing item is composed of."^^xsd:string ;
                       
                       rdfs:subPropertyOf dbp:composition ;
                       
                       rdfs:domain dbr:Clothing ;
                       
                       rdfs:range :ClothingMaterial .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasTexture

:hasTexture rdf:type owl:ObjectProperty ;
            rdfs:label "Has texture" ;
            rdfs:comment "Specifies the texture of the clothing item."^^xsd:string ;
            
            rdfs:domain dbr:Clothing ;
            
            rdfs:range :ClothingTexture .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isManufacteredBy

:isManufacteredBy rdf:type owl:AsymmetricProperty ,
                           owl:FunctionalProperty ,
                           owl:ObjectProperty ;
                  
                  rdfs:label "Is manufactured by"^^xsd:string ;
                  
                  rdfs:domain dbr:Clothing ;
                  
                  rdfs:range gr:BusinessEntity .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForDressingCode

:isSuitableForDressingCode rdf:type owl:ObjectProperty ;
                           
                           rdfs:label "Is suitable for dressing code"^^xsd:string ;
                           
                           rdfs:comment "This property specifis the dressing code for which a particular clothing item and/or class is/are suitable for."^^xsd:string ;
                           
                           rdfs:domain dbr:Clothing ;
                           
                           rdfs:range dbr:Dress_code ,
                                      :ClothingStyle ;
                           
                           owl:inverseOf :dressingCodeHasSuitableClothingForDressing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForDressingForReligion

:isSuitableForDressingForReligion rdf:type owl:ObjectProperty ;
                                  
                                  rdfs:label "Is suitable for dressing for religion"^^xsd:string ;
                                  
                                  rdfs:comment "This property specifis the religion for which a particular clothing item and/or class is/are suitable for."^^xsd:string ;
                                  
                                  rdfs:domain dbr:Clothing ;
                                  
                                  rdfs:range dbr:Religion ;
                                  
                                  owl:inverseOf :religionHasSuitableClothingForDressing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedAtEvent

:isSuitableToBeDressedAtEvent rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "Is suitable to be dressed at event"^^xsd:string ;
                              
                              rdfs:comment "This property specifies the Event for which a particular clothing item and/or class is/are suitable for."^^xsd:string ;
                              
                              rdfs:range dbo:Event ;
                              
                              rdfs:domain dbr:Clothing ;
                              
                              owl:inverseOf :eventHasSuitableClothingForDressing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedByBodyShape

:isSuitableToBeDressedByBodyShape rdf:type owl:ObjectProperty ;
                                  
                                  rdfs:label "Is suitable to be dressed by body shape"^^xsd:string ;
                                  
                                  rdfs:comment "Stipulates for which body shape a particular clothing item and/or class is/are suitable for."^^xsd:string ;
                                  
                                  rdfs:domain dbr:Clothing ;
                                  
                                  rdfs:range :BodyShape ;
                                  
                                  owl:inverseOf :bodyShapeHasSuitableClothingForDressing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedByGenre

:isSuitableToBeDressedByGenre rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "Is suitable to be dressed by genre"^^xsd:string ;
                              
                              rdfs:comment "This property specifies the genre(s) for which a clothing item is suitable for. We need to make this general enough since some clothing items can be suitable for both sexes."^^xsd:string ;
                              
                              rdfs:domain dbr:Clothing ;
                              
                              rdfs:range dbr:Genre ;
                              
                              owl:inverseOf :genreHasSuitableClothingForDressing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedInSeason

:isSuitableToBeDressedInSeason rdf:type owl:ObjectProperty ;
                               
                               rdfs:label "Is suitable to be dressed in season"^^xsd:string ;
                               
                               rdfs:comment "This property specifies the season for which a particular clothing item and/or class is/are suitable for."^^xsd:string ;
                               
                               rdfs:domain dbr:Clothing ;
                               
                               rdfs:range dbr:Season ;
                               
                               owl:inverseOf :seasonHasSuitableClothingForDressing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedOnWeather

:isSuitableToBeDressedOnWeather rdf:type owl:ObjectProperty ;
                                
                                rdfs:label "Is suitable to be dressed in weather conditions"^^xsd:string ;
                                
                                rdfs:comment "This property specifis the weather conditions for which a particular clothing item and/or class is/are suitable for."^^xsd:string ;
                                
                                rdfs:domain dbr:Clothing ;
                                
                                rdfs:range dbr:Weather ;
                                
                                owl:inverseOf :weatherHasSuitableClothingForDressing .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#religionHasSuitableClothingForDressing

:religionHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                        
                                        rdfs:label "Religion has suitable clothing for dressing"^^xsd:string ;
                                        
                                        rdfs:comment "This property specifis the clothing that is suitable for a particular religion."^^xsd:string ;
                                        
                                        rdfs:range dbr:Clothing ;
                                        
                                        rdfs:domain dbr:Religion .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#seasonHasSuitableClothingForDressing

:seasonHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                      
                                      rdfs:label "Season has suitable clothing for dressing"^^xsd:string ;
                                      
                                      rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular season."^^xsd:string ;
                                      
                                      rdfs:range dbr:Clothing ;
                                      
                                      rdfs:domain dbr:Season .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#weatherHasSuitableClothingForDressing

:weatherHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                       
                                       rdfs:label "Weather has suitable clothing for dressing"^^xsd:string ;
                                       
                                       rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular weather condition."^^xsd:string ;
                                       
                                       rdfs:range dbr:Clothing ;
                                       
                                       rdfs:domain dbr:Weather .





#################################################################
#
#    Classes
#
#################################################################


###  http://dbpedia.org/ontology/Event

dbo:Event rdf:type owl:Class ;
          
          owl:disjointUnionOf ( :BusinessMeeting
                                :Club
                                :Date
                                :Funneral
                                :PyjamaParty
                              ) .



###  http://dbpedia.org/resource/Clothing

dbr:Clothing rdf:type owl:Class ;
             
             rdfs:label "Clothing"^^xsd:string ;
             
             rdfs:comment "Represents a clothing item (e.g. Skirt, Jeans, Shoes, etc.) or a collection of clothing items (e.g. a suit -- which is formed from multiple items.). We need to abstract the posibility that a clothing item can be formed from one or multiple clothing items."^^xsd:string ;
             
             owl:disjointUnionOf ( :Accessory
                                   :Blazer
                                   :Coat
                                   :Dress
                                   :Gloves
                                   :Jeans
                                   :Pyjama
                                   :Raincoat
                                   :Shirt
                                   :Shoes
                                   :Skirt
                                   :Suit
                                   :TShirt
                                   :Top
                                   :Trousers
                                   :Underwear
                                 ) .



###  http://dbpedia.org/resource/Clothing_sizes

dbr:Clothing_sizes rdf:type owl:Class .



###  http://dbpedia.org/resource/Color

dbo:Colour rdf:type owl:Class .



###  http://dbpedia.org/resource/Dress_code

dbr:Dress_code rdf:type owl:Class .



###  http://dbpedia.org/resource/Female

dbr:Female rdf:type owl:Class ;
           rdfs:label "Female"@en ;
           owl:disjointWith dbr:Male .



###  http://dbpedia.org/resource/Genre

dbr:Genre rdf:type owl:Class ;
          
          owl:disjointUnionOf ( dbr:Female
                                dbr:Male
                              ) .



###  http://dbpedia.org/resource/Male

dbr:Male rdf:type owl:Class ;
	rdfs:label "Male"@en .



###  http://dbpedia.org/resource/Raw_material

dbr:Raw_material rdf:type owl:Class .



###  http://dbpedia.org/resource/Religion

dbr:Religion rdf:type owl:Class .



###  http://dbpedia.org/resource/Season

dbr:Season rdf:type owl:Class .



###  http://dbpedia.org/resource/Shape

dbr:Shape rdf:type owl:Class .



###  http://dbpedia.org/resource/Weather

dbr:Weather rdf:type owl:Class ;
            
            rdfs:subClassOf owl:Thing .



###  http://purl.org/goodrelations/v1#BusinessEntity

gr:BusinessEntity rdf:type owl:Class .



###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Accessory

:Accessory rdf:type owl:Class ;
           
